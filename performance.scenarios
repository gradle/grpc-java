nonAbiChange {
  tasks = ["assemble"]
  apply-non-abi-change-to = "api/src/main/java/io/grpc/Attributes.java"
  bazel {
    targets = ["build", "--nogenerate_json_trace_profile", "//..."]
  }
}

abiChange {
  tasks = ["assemble"]
  apply-abi-change-to = "api/src/main/java/io/grpc/Attributes.java"
  bazel {
    targets = ["build", "--nogenerate_json_trace_profile", "//..."]
  }
}
